{
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "ES5",
    "module": "commonjs",
    "moduleResolution": "node",
    "noResolve": false,
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react-jsx"
  },
  "exclude": [
    "./node_modules/**/*"
  ]
}