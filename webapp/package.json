{
  "name": "image-share-app-frontend",
  "version": "0.1.0",
  "description": "the web frontend for the image share app",
  "source": "src/index.html",
  "browserslist": "> 5%, last 2 versions, not dead",
  "scripts": {
    "cleanup": "rm -rf .parcel-cache && rm -rf ./dist && rm -rf ./node_modules && npm i",
    "build:server": "npx tsc --project src/webserver/tsconfig.json",
    "build:webapp": "parcel build ./src/index.html --dist-dir ./dist/webapp",
    "build": "npm run build:server && npm run build:webapp",
    "start:dev": "parcel -p 1234",
    "start": "node ./dist/webserver/server.js"
  },
  "author": "Lukas Aichbauer, MSc <l.aichbauer@gmail.com> (https://lukasaichbauer.com)",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.1.3",
    "body-parser": "^1.20.1",
    "dayjs": "^1.11.6",
    "express": "^4.18.2",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "react-toastify": "^9.1.1",
    "react-webcam": "^7.0.1",
    "socket.io-client": "^4.5.4",
    "unstated-next": "^1.1.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.8.3",
    "@parcel/transformer-typescript-tsc": "^2.8.3",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.13",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^5.5.0",
    "crypto-browserify": "^3.12.0",
    "events": "^3.3.0",
    "parcel": "^2.8.3",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.19",
    "process": "^0.11.10",
    "punycode": "^1.4.1",
    "querystring-es3": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "string_decoder": "^1.3.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.5",
    "url": "^0.11.0",
    "util": "^0.12.5"
  }
}
